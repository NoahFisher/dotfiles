{
  "app/resources/*.rb": {
    "type": "resource"
  },
  "app/presenters/*.rb": {
    "type": "presenter"
  },
  "config/initializers/sidekiq.rb": {
    "type": "sidekiq"
  },
  "frontend/app/adapters/*.js": {
    "alternate": "frontend/app/serializers/{}.js",
    "type": "adapter"
  },
  "frontend/app/components/*.js": {
    "alternate": "frontend/app/templates/components/{}.hbs",
    "type": "component"
  },
  "frontend/app/components/*/component.js": {
    "alternate": "frontend/app/components/{}/template.hbs",
    "type": "component"
  },
  "frontend/app/initializers/*.js": {
    "type": "initializer"
  },
  "frontend/app/models/*.js": {
    "alternate": "frontend/app/adapters/{}.js",
    "type": "emodel"
  },
  "frontend/app/serializers/*.js": {
    "alternate": "frontend/app/models/{}.js",
    "type": "serializer"
  },
  "frontend/app/services/*.js": {
    "type": "service"
  },
  "frontend/app/routes/*.js": {
    "alternate": ["frontend/app/templates/{}.hbs", "frontend/app/controllers/{}.js"],
    "type": "route"
  },
  "frontend/app/templates/*.hbs": {
    "alternate": ["frontend/app/controllers/{}.js", "frontend/app/routes/{}.js"],
    "type": "template"
  },
  "frontend/app/controllers/*.js": {
    "alternate": "frontend/app/templates/{}.hbs",
    "type": "controller"
  },
  "frontend/app/templates/components/*.hbs": {
    "alternate": "frontend/app/components/{}.js",
    "type": "ctemplate"
  },
  "spec/acceptance/*.rb": {
    "type": "aspec"
  },
  "spec/factories/*.rb": {
    "alternate": "app/models/{}.rb",
    "type": "factory"
  }
}
